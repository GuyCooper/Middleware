<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>

  <nlog throwExceptions="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="fileLogger" xsi:type="File" layout="${longdate} ${callsite} ${level} ${message}" fileName="MiddlewareTestWindowsApp.log"></target>
      <!--${logger}.txt-->
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" maxlevel="Error" writeTo="fileLogger"></logger>
    </rules>
  </nlog>
</configuration>
